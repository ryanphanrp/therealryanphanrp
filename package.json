{
  "name": "therealryanphan",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "clean": "turbo run clean",
    "clean:kill": "turbo run clean && npx npkill",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier '**/*.{js,ts,css,scss,jsx,tsx}' --write",
    "ui:add": "pnpm --filter ui ui:add",
    "prepare": "husky install",
    "check:types": "turbo typecheck"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tsconfig": "workspace:*",
    "turbo": "^1.13.0"
  }
}
